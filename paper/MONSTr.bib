@article{calcara_whydroneshave_2022,
  title = {Why {{Drones Have Not Revolutionized War}}: {{The Enduring Hider-Finder Competition}} in {{Air Warfare}}},
  shorttitle = {Why {{Drones Have Not Revolutionized War}}},
  author = {Calcara, Antonio and Gilli, Andrea and Gilli, Mauro and Marchetti, Raffaele and Zaccagnini, Ivan},
  year = {2022},
  month = apr,
  journal = {International Security},
  volume = {46},
  number = {4},
  pages = {130--171},
  issn = {0162-2889},
  doi = {10.1162/isec_a_00431},
  abstract = {According to the accepted wisdom in security studies, unmanned aerial vehicles, also known as drones, have revolutionizing effects on war and world politics. Drones allegedly tilt the military balance in favor of the offense, reduce existing asymmetries in military power between major and minor actors, and eliminate close combat from modern battlefields. A new theory about the hider-finder competition between air penetration and air defense shows that drones are vulnerable to air defenses and electronic warfare systems, and that they require support from other force structure assets to be effective. This competition imposes high costs on those who fail to master the set of tactics, techniques, procedures, technologies, and capabilities necessary to limit exposure to enemy fire and to detect enemy targets. Three conflicts that featured extensive employment of drones\textemdash the Western Libya military campaign of the second Libyan civil war (2019\textendash 2020), the Syrian civil war (2011\textendash 2021), and the Armenia-Azerbaijan conflict over Nagorno-Karabakh (2020)\textemdash probe the mechanisms of the theory. Drones do not by themselves produce the revolutionary effects that many have attributed to them.},
  file = {/home/andresgannon/Zotero/storage/HP5EU924/Calcara et al_2022_Why Drones Have Not Revolutionized War.pdf;/home/andresgannon/Zotero/storage/2XBZ49CU/Why-Drones-Have-Not-Revolutionized-War-The.html}
}

